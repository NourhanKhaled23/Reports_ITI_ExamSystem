        <CommandText>SP_Report_StudentsByBranch</CommandText>
        <QueryParameters>
          <QueryParameter Name="@BranchId">
            <Value>=Parameters!BranchId.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="StudentId"><DataField>StudentId</DataField></Field>
        <Field Name="StudentName"><DataField>StudentName</DataField></Field>
        <Field Name="Gender"><DataField>Gender</DataField></Field>
        <Field Name="EnrollmentDate"><DataField>EnrollmentDate</DataField></Field>
