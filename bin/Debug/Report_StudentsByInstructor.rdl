<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ITIExamDB">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=.;Initial Catalog=ITIExamintaionSystem;Integrated Security=True</ConnectString>
      </ConnectionProperties>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="InstructorList">
      <Query>
        <DataSourceName>ITIExamDB</DataSourceName>
        <CommandType>StoredProcedure</CommandType>
        <CommandText>SP_Report_GetAllInstructors</CommandText>
      </Query>
      <Fields>
        <Field Name="Id"><DataField>Id</DataField></Field>
        <Field Name="FullName"><DataField>FullName</DataField></Field>
      </Fields>
    </DataSet>
    <DataSet Name="StudentsByInstructor">
      <Query>
        <DataSourceName>ITIExamDB</DataSourceName>
        <CommandType>StoredProcedure</CommandType>
        <CommandText>SP_Report_StudentsByInstructor</CommandText>
        <QueryParameters>
          <QueryParameter Name="@InstructorId">
            <Value>=Parameters!InstructorId.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="InstructorId"><DataField>InstructorId</DataField></Field>
        <Field Name="InstructorName"><DataField>InstructorName</DataField></Field>
        <Field Name="CourseName"><DataField>CourseName</DataField></Field>
        <Field Name="StudentId"><DataField>StudentId</DataField></Field>
        <Field Name="StudentName"><DataField>StudentName</DataField></Field>
        <Field Name="EnrollmentDate"><DataField>EnrollmentDate</DataField></Field>
        <Field Name="CourseStatus"><DataField>CourseStatus</DataField></Field>
        <Field Name="FinalGrade"><DataField>FinalGrade</DataField></Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="InstructorId">
      <DataType>Integer</DataType>
      <Prompt>Select Instructor</Prompt>
      <ValidValues>
        <DataSetReference>
          <DataSetName>InstructorList</DataSetName>
          <ValueField>Id</ValueField>
          <LabelField>FullName</LabelField>
        </DataSetReference>
      </ValidValues>
    </ReportParameter>
  </ReportParameters>
  <ReportSections>
    <ReportSection>
      <Body>
        <ReportItems>
          <Textbox Name="Title"><Top>0.1in</Top><Left>0in</Left><Height>0.4in</Height><Width>7in</Width>
            <Paragraphs><Paragraph><TextRuns><TextRun><Value>Students by Instructor Report</Value>
              <Style><FontSize>18pt</FontSize><FontWeight>Bold</FontWeight><Color>#1e3a5f</Color></Style>
            </TextRun></TextRuns><Style><TextAlign>Center</TextAlign></Style></Paragraph></Paragraphs>
          </Textbox>
          <Textbox Name="InstructorInfo"><Top>0.55in</Top><Left>0in</Left><Height>0.3in</Height><Width>7in</Width>
            <Paragraphs><Paragraph><TextRuns><TextRun><Value>=First(Fields!InstructorName.Value, "StudentsByInstructor")</Value>
              <Style><FontSize>12pt</FontSize><FontWeight>Bold</FontWeight><Color>#4a4a4a</Color></Style>
            </TextRun></TextRuns><Style><TextAlign>Center</TextAlign></Style></Paragraph></Paragraphs>
          </Textbox>
          <Tablix Name="StudentsTable">
            <TablixBody>
              <TablixColumns>
                <TablixColumn><Width>1.8in</Width></TablixColumn>
                <TablixColumn><Width>1.8in</Width></TablixColumn>
                <TablixColumn><Width>1in</Width></TablixColumn>
                <TablixColumn><Width>1in</Width></TablixColumn>
                <TablixColumn><Width>1in</Width></TablixColumn>
              </TablixColumns>
              <TablixRows>
                <TablixRow><Height>0.3in</Height>
                  <TablixCells>
                    <TablixCell><CellContents><Textbox Name="H1"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Course</Value><Style><FontWeight>Bold</FontWeight><Color>White</Color></Style></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>#1e3a5f</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H2"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Student Name</Value><Style><FontWeight>Bold</FontWeight><Color>White</Color></Style></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>#1e3a5f</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H3"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Enrolled</Value><Style><FontWeight>Bold</FontWeight><Color>White</Color></Style></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>#1e3a5f</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H4"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Status</Value><Style><FontWeight>Bold</FontWeight><Color>White</Color></Style></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>#1e3a5f</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H5"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Grade</Value><Style><FontWeight>Bold</FontWeight><Color>White</Color></Style></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>#1e3a5f</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                  </TablixCells>
                </TablixRow>
                <TablixRow><Height>0.25in</Height>
                  <TablixCells>
                    <TablixCell><CellContents><Textbox Name="C1"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!CourseName.Value</Value><Style><FontWeight>SemiBold</FontWeight></Style></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>=IIF(RowNumber(Nothing) Mod 2 = 0, "#f2f2f2", "White")</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="C2"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!StudentName.Value</Value></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>=IIF(RowNumber(Nothing) Mod 2 = 0, "#f2f2f2", "White")</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="C3"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Format(Fields!EnrollmentDate.Value, "dd/MM/yyyy")</Value></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>=IIF(RowNumber(Nothing) Mod 2 = 0, "#f2f2f2", "White")</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="C4"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!CourseStatus.Value</Value></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>=IIF(RowNumber(Nothing) Mod 2 = 0, "#f2f2f2", "White")</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="C5"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!FinalGrade.Value</Value></TextRun></TextRuns></Paragraph></Paragraphs><Style><BackgroundColor>=IIF(RowNumber(Nothing) Mod 2 = 0, "#f2f2f2", "White")</BackgroundColor></Style></Textbox></CellContents></TablixCell>
                  </TablixCells>
                </TablixRow>
              </TablixRows>
            </TablixBody>
            <TablixColumnHierarchy><TablixMembers><TablixMember/><TablixMember/><TablixMember/><TablixMember/><TablixMember/></TablixMembers></TablixColumnHierarchy>
            <TablixRowHierarchy><TablixMembers><TablixMember><KeepWithGroup>After</KeepWithGroup></TablixMember><TablixMember><Group Name="Details"/></TablixMember></TablixMembers></TablixRowHierarchy>
            <DataSetName>StudentsByInstructor</DataSetName>
            <Top>1in</Top><Left>0.25in</Left><Height>0.55in</Height><Width>6.6in</Width>
          </Tablix>
        </ReportItems>
        <Height>2in</Height>
      </Body>
      <Width>7.5in</Width>
      <Page><LeftMargin>0.5in</LeftMargin><RightMargin>0.5in</RightMargin><TopMargin>0.5in</TopMargin><BottomMargin>0.5in</BottomMargin></Page>
    </ReportSection>
  </ReportSections>
</Report>
